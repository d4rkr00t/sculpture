{
  "name": "sculpture-cli",
  "napi": {
    "name": "sculpture-cli",
    "triples": {}
  },
  "bin": {
    "sculpture": "dist/cli.js"
  },
  "dependencies": {
    "@node-rs/helper": "^1.0.0",
    "@opaline/core": "^0.5.0"
  },
  "devDependencies": {
    "@napi-rs/cli": "^1.0.0",
    "typescript": "^4.1.3",
    "lint-staged": "^10.5.3",
    "pre-commit": "^1.2.2",
    "prettier": "^2.2.1",
    "watch": "^1.0.2"
  },
  "workspaces": [
    "example/*/*"
  ],
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "prepublishOnly": "napi prepublish -t npm",
    "version": "napi version",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run build:cli && npm run build:native",
    "build:cli": "opaline build",
    "build:native": "napi build --platform --release",
    "build:native:debug": "napi build --platform --release",
    "dev": "opaline dev",
    "dev:native": "watch 'npm run build:native:debug' ./src",
    "typecheck": "tsc",
    "lint:staged": "lint-staged",
    "prepare": "npm run typecheck"
  }
}
